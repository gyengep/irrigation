	DocumentView-clean \
	DS18B20-clean \
	DtoReaderWriter-clean \
	Logger-clean \
	Logic-clean \
	Model-clean \
	RestView-clean \
	Schedulers-clean \
	Temperature-clean \
	TimerView-clean \
	Utils-clean \
	Valves-clean \
	WebServer-clean

