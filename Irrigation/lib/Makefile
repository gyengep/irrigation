
.PHONY: all
all: \
	DocumentView-all \
	DS18B20-all \
	DtoReaderWriter-all \
	Logger-all \
	Logic-all \
	Model-all \
	RestView-all \
	Schedulers-all \
	Temperature-all \
	TimerView-all \
	Utils-all \
	Valves-all \
